{
  "presets": [
    [
      "@babel/env",
      {
        "corejs": {
          "version": 3
        },
        "useBuiltIns": "usage"
      }
    ],
    "@babel/typescript"
  ],
  "plugins": ["tsconfig-paths-module-resolver"],
  "env": {
    "production": {
      "ignore": ["*.d.ts"]
    },
    "development": {
      "ignore": ["*.d.ts"]
    }
  }
}
